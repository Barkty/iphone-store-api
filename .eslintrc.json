{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": ["airbnb-base", "prettier", "plugin:import/errors", "plugin:import/warnings"],
    "globals": {},
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "allowImportExportEverywhere": true,
    },
    "rules": {
        "no-underscore-dangle": "off",
        "no-param-reassign": "off",
        "comma-dangle": "off",
        "max-len": [
            "error",
            {
                "code": 120
            }
        ],
        "no-unused-vars": 1,
        "no-console": 1,
        "no-plusplus": 0,
        "import/prefer-default-export": "off",
        "eqeqeq": "off",
        "object-shorthand": "off",
        "func-names": "off"
    }
}
